{
  "enabled": true,
  "local_model": "qwen3:8b",
  "cloud_model": "deepseek/deepseek-v3.2",
  "use_cloud_for_complex_tasks": true,
  "complexity_threshold": 0.7,
  "min_budget_for_cloud": 0.1,
  "knowledge_graph_path": "data/local_state/memory/knowledge_graph.db",
  "components": {
    "core": {
      "enabled": true,
      "model": "qwen3:8b",
      "cost_multiplier": 0.01,
      "speed_score": 3.0,
      "capabilities": ["reflection", "routine", "memory_access", "basic_reasoning"]
    },
    "strategist": {
      "enabled": true,
      "model": "deepseek/deepseek-v3.2",
      "cost_multiplier": 1.0,
      "speed_score": 0.5,
      "capabilities": ["deep_reasoning", "planning", "research", "creative_thinking", "complex_problem_solving"]
    },
    "math_reasoner": {
      "enabled": true,
      "model": "core",
      "cost_multiplier": 0.1,
      "speed_score": 2.0,
      "capabilities": ["mathematical_reasoning", "calculations", "proofs"]
    },
    "code_generator": {
      "enabled": true,
      "model": "core",
      "cost_multiplier": 0.1,
      "speed_score": 2.0,
      "capabilities": ["code_generation", "refactoring", "debugging"]
    },
    "self_reflection": {
      "enabled": true,
      "model": "core",
      "cost_multiplier": 0.05,
      "speed_score": 1.5,
      "capabilities": ["introspection", "identity_maintenance", "value_alignment"]
    }
  },
  "knowledge_graph": {
    "type": "sqlite",
    "path": "data/local_state/memory/knowledge_graph.db",
    "auto_sync": true,
    "sync_interval_seconds": 300
  },
  "performance": {
    "target_response_time_seconds": 3.0,
    "max_local_parameters": 3000000000,
    "max_module_parameters": 500000000,
    "parallel_processing": true,
    "cache_enabled": true,
    "cache_ttl_seconds": 3600
  },
  "learning": {
    "distillation_enabled": true,
    "distillation_batch_size": 10,
    "knowledge_extraction_enabled": true,
    "auto_update_interval_hours": 24,
    "training_data_path": "data/local_state/memory/training_data/"
  },
  "debug": {
    "log_cognitive_decisions": true,
    "log_component_usage": true,
    "log_knowledge_updates": true,
    "performance_monitoring": true
  }
}